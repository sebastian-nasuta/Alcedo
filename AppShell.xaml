<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Alcedo.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Alcedo"
    xmlns:pages="clr-namespace:Alcedo.Pages"
    Title="Alcedo"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Image tag generator"
        ContentTemplate="{DataTemplate pages:ImageTaggingPage}"
        Route="ImageTaggingPage" />

    <ShellContent
        Title="Barcode scanner"
        ContentTemplate="{DataTemplate pages:BarcodeScannerPage}"
        Route="BarcodeScannerPage" />

    <ShellContent
        Title="Settings"
        ContentTemplate="{DataTemplate pages:SettingsPage}"
        Route="SettingsPage" />

</Shell>
